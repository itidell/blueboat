{
  "expo": {
    "name": "BlueBoatApp",
    "slug": "BlueBoatApp",
    "scheme": "blueboat",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["DEFAULT", "BROWSABLE"],
          "data": {
            "scheme": "blueboat"
          }
        }
      ]
    },
    "web": {
    },
    "plugins": [
      "expo-font"
    ]
  }
}
